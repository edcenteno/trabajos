{
    "name": "jossmp/datos-peru",
    "description": "Clase para realizar consultas a reniec, essalud, ministrio del trabajo con el numero de DNI",
    "type": "library",
    "keywords": ["Reniec", "EsSalud", "Mintra", "Peru", "PHP", "Consulta", "cURL"],
    "homepage" : "https://github.com/JossMP/datos-peru",
    "license": "GPL-3.0-or-later",
    "minimum-stability": "dev",
    "optimize-autoloader": true,
    "authors": [
        {
            "name" 		: "Josue Mazco Puma",
            "email" 	: "jossmp@gmail.com",
            "homepage" 	: "https://geekdev.ml/",
            "role" 		: "Developers"
        }
    ],
    "require": {
        "ext-curl" : "*",
        "lib-curl" : "*",
        "purekid/mongodm": "dev-master"
    },
    "require-dev": {
        "php" 	: ">=5.2"
    },
    "autoload": {
		"psr-4": {
			"Reniec\\" 	: "src/reniec/",
			"MinTra\\" 	: "src/mintra/",
			"EsSalud\\" : "src/essalud/",
            "CURL\\"    : "src/",
			"Modelo\\" 	: "modelo/"
		}
	}
}
